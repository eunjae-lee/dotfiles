// Place your key bindings in this file to override the defaultsauto[]
[
  {
    "key": "cmd+up",
    "command": "cursorTop",
    "when": "textInputFocus"
  },
  {
    "key": "cmd+down",
    "command": "cursorBottom",
    "when": "textInputFocus"
  },
  {
    "key": "cmd+end",
    "command": "-cursorBottom",
    "when": "textInputFocus"
  },
  // {
  //   "key": "shift shift",
  //   "command": "workbench.action.terminal.toggleTerminal",
  //   "when": "terminal.active"
  // },
  {
    "key": "shift+escape",
    "command": "-workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "cmd+k cmd+s",
    "command": "workbench.action.files.saveWithoutFormatting"
  },
  {
    "key": "cmd+k s",
    "command": "-workbench.action.files.saveWithoutFormatting"
  },
  {
    "key": "ctrl+shift+g",
    "command": "-cursorUndo",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k",
    "command": "-git.commitAll",
    "when": "!inDebugMode && !terminalFocus"
  },
  {
    "key": "cmd+k",
    "command": "workbench.action.terminal.clear",
    "when": "terminalFocus"
  },
  {
    "key": "cmd+k",
    "command": "-workbench.action.terminal.clear",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "cmd+s",
    "command": "-workbench.action.files.save"
  },
  {
    "key": "cmd+s",
    "command": "workbench.action.files.saveFiles"
  },
  {
    "key": "cmd+s",
    "command": "-workbench.action.files.saveAll"
  },
  {
    "key": "shift+cmd+e",
    "command": "-workbench.view.explorer",
    "when": "viewContainer.workbench.view.explorer.enabled"
  },
  {
    "key": "shift+cmd+e",
    "command": "workbench.explorer.fileView.focus"
  },
  {
    "key": "cmd+e",
    "command": "-actions.findWithSelection"
  },
  {
    "key": "cmd+e",
    "command": "workbench.action.openRecent"
  },
  {
    "key": "ctrl+r",
    "command": "-workbench.action.openRecent"
  },
  {
    "key": "cmd+n",
    "command": "-editor.action.sourceAction",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+n",
    "command": "-editor.action.sourceAction",
    "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+cmd+t",
    "command": "-workbench.action.closeOtherEditors"
  },
  {
    "key": "cmd+k u",
    "command": "-workbench.action.closeUnmodifiedEditors"
  },
  {
    "key": "alt+cmd+y",
    "command": "workbench.action.closeUnmodifiedEditors"
  },
  {
    "key": "alt+cmd+t",
    "command": "workbench.action.closeOtherEditors"
  },
  {
    "key": "shift+cmd+k",
    "command": "-editor.action.deleteLines",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "shift+cmd+k",
    "command": "editor.action.showDefinitionPreviewHover"
  },
  {
    "key": "shift+cmd+m",
    "command": "editor.emmet.action.wrapWithAbbreviation"
  },
  {
    "key": "shift+cmd+m",
    "command": "-workbench.actions.view.problems",
    "when": "workbench.panel.markers.view.active"
  },
  {
    "key": "ctrl+cmd+g",
    "command": "-editor.action.selectHighlights",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+shift+alt+cmd+enter",
    "command": "runCommands",
    "args": {
      "commands": [
        "workbench.action.closePanel",
        "workbench.action.closeSidebar"
      ]
    },
    "when": "editorFocus"
  },
  {
    "key": "ctrl+shift+alt+cmd+enter",
    "command": "runCommands",
    "args": {
      "commands": [
        "workbench.action.toggleMaximizedPanel",
        "workbench.action.closeSidebar"
      ]
    },
    "when": "inDebugRepl && panelAlignment == 'center' || problemFocus && panelAlignment == 'center' || terminalFocus && panelAlignment == 'center' || focusedView == 'workbench.panel.output' && panelAlignment == 'center' || inDebugRepl && panelPosition != 'bottom' && panelPosition != 'top' || problemFocus && panelPosition != 'bottom' && panelPosition != 'top' || terminalFocus && panelPosition != 'bottom' && panelPosition != 'top' || focusedView == 'workbench.panel.output' && panelPosition != 'bottom' && panelPosition != 'top'"
  },
  {
    "key": "shift+cmd+'",
    "command": "-workbench.action.toggleMaximizedPanel",
    "when": "inDebugRepl && panelAlignment == 'center' || problemFocus && panelAlignment == 'center' || terminalFocus && panelAlignment == 'center' || focusedView == 'workbench.panel.output' && panelAlignment == 'center' || inDebugRepl && panelPosition != 'bottom' && panelPosition != 'top' || problemFocus && panelPosition != 'bottom' && panelPosition != 'top' || terminalFocus && panelPosition != 'bottom' && panelPosition != 'top' || focusedView == 'workbench.panel.output' && panelPosition != 'bottom' && panelPosition != 'top'"
  },
  {
    "key": "cmd+k",
    "command": "-git.commitAll",
    "when": "!inDebugMode && !operationInProgress && !terminalFocus"
  },
  {
    "key": "cmd+l",
    "command": "-workbench.action.gotoLine"
  },
  {
    "key": "cmd+k",
    "command": "aipopup.action.modal.generate",
    "when": "!composerBarIsVisible && !composerControlPanelIsVisible"
  },
  {
    "key": "cmd+l",
    "command": "aichat.focuschatpaneaction"
  },
  {
    "key": "shift+cmd+l",
    "command": "-editor.action.selectHighlights",
    "when": "editorFocus"
  },
  {
    "key": "shift+cmd+l",
    "command": "-selectAllSearchEditorMatches",
    "when": "inSearchEditor"
  },
  {
    "key": "shift shift",
    "command": "-workbench.action.quickOpen"
  }
]
